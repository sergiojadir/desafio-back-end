<%= form_for arquivo_cnab do |f| %>
	<div class="form-group row">
		<%= f.label :arquivo, class: 'col-sm-2 col-form-label' %>
		<div class="col-sm-10">
  		<%= f.attachment_field :arquivo, class: 'form-control' %>
  	</div>
  </div>

  <div class="form-group row">
    <div class="col-sm-10">
  		<%= f.submit "Enviar", class: 'btn btn-primary' %>
  		<%= link_to 'Voltar', arquivo_cnabs_path, class: 'btn btn-danger' %>
  	</div>
  </div>
<% end %>